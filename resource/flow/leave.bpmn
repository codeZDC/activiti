    </sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow5" sourceRef="manKey" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="bossKey" targetRef="endevent3"></sequenceFlow>
